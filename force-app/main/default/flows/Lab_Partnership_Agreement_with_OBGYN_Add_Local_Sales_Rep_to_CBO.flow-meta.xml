<?xml version="1.0" encoding="UTF-8"?>
<Flow xmlns="http://soap.sforce.com/2006/04/metadata">
    <apiVersion>49.0</apiVersion>
    <areMetricsLoggedToDataCloud>false</areMetricsLoggedToDataCloud>
    <constants>
        <name>conPartnershipAgreement</name>
        <dataType>String</dataType>
        <value>
            <stringValue>Agreement</stringValue>
        </value>
    </constants>
    <decisions>
        <description>Is there a Lab that meets the criteria -- there is a Partnership Agreement and the Start Date is less than the Create Date for the Lab.</description>
        <name>Is_there_a_Partnership_Agreement</name>
        <label>Is there a Partnership Agreement</label>
        <locationX>359</locationX>
        <locationY>70</locationY>
        <defaultConnectorLabel>Default Outcome</defaultConnectorLabel>
        <rules>
            <name>Partnership_Agreement</name>
            <conditionLogic>and</conditionLogic>
            <conditions>
                <leftValueReference>varLabID</leftValueReference>
                <operator>IsNull</operator>
                <rightValue>
                    <booleanValue>false</booleanValue>
                </rightValue>
            </conditions>
            <connector>
                <targetReference>Update_CBO_with_Local_Rep</targetReference>
            </connector>
            <label>Partnership Agreement</label>
        </rules>
    </decisions>
    <formulas>
        <name>formLocalSalesRep</name>
        <dataType>String</dataType>
        <expression>if ({!varOBGYNTerritory} = &quot;CA - Los Angeles&quot;, &quot;Kimberly Schienberg&quot;,
if ({!varOBGYNTerritory} = &quot;NY - Manhattan&quot;, &quot;Brian Packard&quot;,
if ({!varOBGYNTerritory} = &quot;CA - Orange/Riverside&quot;, &quot;Melissa Keil&quot;,
null)))</expression>
    </formulas>
    <formulas>
        <name>formLSRAddedPostAutoBill</name>
        <dataType>Boolean</dataType>
        <expression>if ({!varAutoBill}=true, true, false)</expression>
    </formulas>
    <formulas>
        <name>formToday</name>
        <dataType>Date</dataType>
        <expression>today()</expression>
    </formulas>
    <interviewLabel>Lab Partnership Agreement with OBGYN Add Local Sales Rep to CBO {!$Flow.CurrentDateTime}</interviewLabel>
    <label>Lab Partnership Agreement with OBGYN Add Local Sales Rep to CBO</label>
    <processMetadataValues>
        <name>BuilderType</name>
        <value>
            <stringValue>LightningFlowBuilder</stringValue>
        </value>
    </processMetadataValues>
    <processMetadataValues>
        <name>OriginBuilderType</name>
        <value>
            <stringValue>LightningFlowBuilder</stringValue>
        </value>
    </processMetadataValues>
    <processType>AutoLaunchedFlow</processType>
    <recordLookups>
        <name>Lookup_Lab</name>
        <label>Lookup Lab</label>
        <locationX>186</locationX>
        <locationY>68</locationY>
        <assignNullValuesIfNoRecordsFound>true</assignNullValuesIfNoRecordsFound>
        <connector>
            <targetReference>Is_there_a_Partnership_Agreement</targetReference>
        </connector>
        <filterLogic>and</filterLogic>
        <filters>
            <field>CreatedDate</field>
            <operator>GreaterThan</operator>
            <value>
                <elementReference>varPartnershipStartDate</elementReference>
            </value>
        </filters>
        <filters>
            <field>Id</field>
            <operator>EqualTo</operator>
            <value>
                <elementReference>varLabID</elementReference>
            </value>
        </filters>
        <filters>
            <field>HCP_Agreement__c</field>
            <operator>Contains</operator>
            <value>
                <elementReference>conPartnershipAgreement</elementReference>
            </value>
        </filters>
        <filters>
            <field>HCP_Territory__c</field>
            <operator>IsNull</operator>
            <value>
                <booleanValue>false</booleanValue>
            </value>
        </filters>
        <object>Lab__c</object>
        <outputAssignments>
            <assignToReference>varOBGYNTerritory</assignToReference>
            <field>HCP_Territory__c</field>
        </outputAssignments>
    </recordLookups>
    <recordUpdates>
        <name>Update_CBO_with_Local_Rep</name>
        <label>Update CBO with Local Rep</label>
        <locationX>622</locationX>
        <locationY>211</locationY>
        <filterLogic>and</filterLogic>
        <filters>
            <field>Id</field>
            <operator>EqualTo</operator>
            <value>
                <elementReference>varCBOID</elementReference>
            </value>
        </filters>
        <inputAssignments>
            <field>LSR_Added_Post_Autobill__c</field>
            <value>
                <elementReference>formLSRAddedPostAutoBill</elementReference>
            </value>
        </inputAssignments>
        <inputAssignments>
            <field>LSR_Populated_from_Lab__c</field>
            <value>
                <elementReference>formToday</elementReference>
            </value>
        </inputAssignments>
        <inputAssignments>
            <field>Local_Sales_Rep__c</field>
            <value>
                <elementReference>formLocalSalesRep</elementReference>
            </value>
        </inputAssignments>
        <object>fw1__Invoice__c</object>
    </recordUpdates>
    <startElementReference>Lookup_Lab</startElementReference>
    <status>Active</status>
    <variables>
        <name>varAutoBill</name>
        <dataType>Boolean</dataType>
        <isCollection>false</isCollection>
        <isInput>true</isInput>
        <isOutput>true</isOutput>
    </variables>
    <variables>
        <name>varCBOID</name>
        <dataType>String</dataType>
        <isCollection>false</isCollection>
        <isInput>true</isInput>
        <isOutput>true</isOutput>
    </variables>
    <variables>
        <name>varLabCreateDate</name>
        <dataType>Date</dataType>
        <isCollection>false</isCollection>
        <isInput>true</isInput>
        <isOutput>true</isOutput>
    </variables>
    <variables>
        <name>varLabID</name>
        <dataType>String</dataType>
        <isCollection>false</isCollection>
        <isInput>true</isInput>
        <isOutput>true</isOutput>
    </variables>
    <variables>
        <name>varOBGYNTerritory</name>
        <dataType>String</dataType>
        <isCollection>false</isCollection>
        <isInput>true</isInput>
        <isOutput>true</isOutput>
    </variables>
    <variables>
        <name>varPartnershipStartDate</name>
        <dataType>Date</dataType>
        <isCollection>false</isCollection>
        <isInput>true</isInput>
        <isOutput>true</isOutput>
    </variables>
    <variables>
        <name>varPartnershipStatus</name>
        <dataType>String</dataType>
        <isCollection>false</isCollection>
        <isInput>true</isInput>
        <isOutput>true</isOutput>
    </variables>
</Flow>
