    <decisions>
        <description>Determine if tracking id and dept number are updated</description>
        <name>Has_Tracking_ID_and_Dept_Number_been_set</name>
        <label>Has Tracking ID and Dept Number been set</label>
        <locationX>248</locationX>
        <locationY>323</locationY>
        <defaultConnector>
            <targetReference>Has_Tracking_ID_and_Dept_Number_been_set_MBD</targetReference>
        </defaultConnector>
        <defaultConnectorLabel>NO - Tracking Not Set and Not Non-MBD</defaultConnectorLabel>
        <rules>
            <name>YES_Non_MBD_and_Tracking_ID_Dept_Number_set</name>
            <conditionLogic>and</conditionLogic>
            <conditions>
                <leftValueReference>$Record.zkfedex__MasterTrackingId__c</leftValueReference>
                <operator>IsNull</operator>
                <rightValue>
                    <booleanValue>false</booleanValue>
                </rightValue>
            </conditions>
            <conditions>
                <leftValueReference>$Record.zkfedex__ReferenceDeptNumber__c</leftValueReference>
                <operator>EqualTo</operator>
                <rightValue>
                    <stringValue>P</stringValue>
                </rightValue>
            </conditions>
            <connector>
                <targetReference>Send_mother_email_when_kit_is_shipped</targetReference>
            </connector>
            <doesRequireRecordChangedToMeetCriteria>true</doesRequireRecordChangedToMeetCriteria>
            <label>YES - Non-MBD and Tracking ID/Dept Number set</label>
        </rules>
    </decisions>
    <decisions>
        <description>Determine if tracking id and dept number are updated for MBD</description>
