            <value>
                <elementReference>varOpportunityID</elementReference>
            </value>
        </filters>
        <inputAssignments>
            <field>Local_Sales_Rep__c</field>
            <value>
                <elementReference>formLocalSalesRep</elementReference>
            </value>
