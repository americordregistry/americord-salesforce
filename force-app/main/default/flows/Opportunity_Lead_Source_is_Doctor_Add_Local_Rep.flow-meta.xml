            </connector>
            <label>Not Null</label>
        </rules>
    </decisions>
    <formulas>
        <name>formLocalSalesRep</name>
        <dataType>String</dataType>
        <expression>if ({!varTerritory} = &quot;CA - Los Angeles&quot;, &quot;Kimberly Schienberg&quot;,
if ({!varTerritory} = &quot;NY - Manhattan&quot;, &quot;Brian Packard&quot;,
if ({!varTerritory} = &quot;CA - Orange/Riverside&quot;, &quot;Melissa Keil&quot;,
null)))</expression>
    </formulas>
