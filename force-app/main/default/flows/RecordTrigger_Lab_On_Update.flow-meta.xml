<?xml version="1.0" encoding="UTF-8"?>
<Flow xmlns="http://soap.sforce.com/2006/04/metadata">
    <apiVersion>56.0</apiVersion>
    <areMetricsLoggedToDataCloud>false</areMetricsLoggedToDataCloud>
    <decisions>
        <description>Check to see if the rollup summary ran and updated the stored values</description>
        <name>Did_Product_Stored_Rollup_Occur</name>
        <label>Did Product Stored Rollup Occur</label>
        <locationX>182</locationX>
        <locationY>323</locationY>
        <defaultConnector>
            <targetReference>Did_WP_Status_Change</targetReference>
        </defaultConnector>
        <defaultConnectorLabel>No - No Rollup</defaultConnectorLabel>
        <rules>
            <name>YES_Rollup_occurred</name>
            <conditionLogic>or</conditionLogic>
            <conditions>
                <leftValueReference>$Record.Cord_Blood_2_0_Stored__c</leftValueReference>
                <operator>IsChanged</operator>
                <rightValue>
                    <booleanValue>true</booleanValue>
                </rightValue>
            </conditions>
            <conditions>
                <leftValueReference>$Record.Cord_Tissue_Stored__c</leftValueReference>
                <operator>IsChanged</operator>
                <rightValue>
                    <booleanValue>true</booleanValue>
                </rightValue>
            </conditions>
            <conditions>
                <leftValueReference>$Record.Exosomes_Stored__c</leftValueReference>
                <operator>IsChanged</operator>
                <rightValue>
                    <booleanValue>true</booleanValue>
                </rightValue>
            </conditions>
            <conditions>
                <leftValueReference>$Record.Placenta_2_0_Amnion_Stored__c</leftValueReference>
                <operator>IsChanged</operator>
                <rightValue>
                    <booleanValue>true</booleanValue>
                </rightValue>
            </conditions>
            <conditions>
                <leftValueReference>$Record.Placenta_Tissue_Stored__c</leftValueReference>
                <operator>IsChanged</operator>
                <rightValue>
                    <booleanValue>true</booleanValue>
                </rightValue>
            </conditions>
            <conditions>
                <leftValueReference>$Record.Maternal_Exosomes_Stored__c</leftValueReference>
                <operator>IsChanged</operator>
                <rightValue>
                    <booleanValue>true</booleanValue>
                </rightValue>
            </conditions>
            <connector>
                <targetReference>Update_CBO_Stored_Values</targetReference>
            </connector>
            <label>YES - Rollup occurred</label>
        </rules>
    </decisions>
    <decisions>
        <description>Check to see if the WP Status Changed</description>
        <name>Did_WP_Status_Change</name>
        <label>Did WP Status Change</label>
        <locationX>182</locationX>
        <locationY>623</locationY>
        <defaultConnector>
            <targetReference>Update_baby_sample_status_fields</targetReference>
        </defaultConnector>
        <defaultConnectorLabel>WP - Status Not Changed</defaultConnectorLabel>
        <rules>
            <name>YES_WP_Status_Changed</name>
            <conditionLogic>and</conditionLogic>
            <conditions>
                <leftValueReference>$Record.DocuSign_Status__c</leftValueReference>
                <operator>IsChanged</operator>
                <rightValue>
                    <booleanValue>true</booleanValue>
                </rightValue>
            </conditions>
            <connector>
                <targetReference>Update_CBO_WP_Status</targetReference>
            </connector>
            <label>YES - WP Status Changed</label>
        </rules>
    </decisions>
