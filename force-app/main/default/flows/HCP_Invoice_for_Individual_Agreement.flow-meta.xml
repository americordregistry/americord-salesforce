    </formulas>
    <formulas>
        <name>formStreet</name>
        <dataType>String</dataType>
        <expression>if ( NOT(ISBLANK({!varOtherStreet})), {!varOtherStreet},{!varMailingStreet})</expression>
    </formulas>
