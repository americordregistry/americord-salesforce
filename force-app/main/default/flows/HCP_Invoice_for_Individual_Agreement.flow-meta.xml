    <decisions>
        <name>Is_there_an_Open_Invoice</name>
        <label>Is there an Open Invoice</label>
        <locationX>359</locationX>
        <locationY>236</locationY>
        <defaultConnectorLabel>Default Outcome</defaultConnectorLabel>
        <rules>
            <name>Open_Invoice_Exists</name>
            <conditionLogic>and</conditionLogic>
            <conditions>
                <leftValueReference>varHCPInvoiceID</leftValueReference>
                <operator>IsNull</operator>
                <rightValue>
                    <booleanValue>false</booleanValue>
                </rightValue>
            </conditions>
            <connector>
                <targetReference>Create_Stipend</targetReference>
            </connector>
            <label>Open Invoice Exists</label>
        </rules>
        <rules>
            <name>Invoice_Does_Not_Exist</name>
            <conditionLogic>and</conditionLogic>
            <conditions>
                <leftValueReference>varHCPInvoiceID</leftValueReference>
                <operator>IsNull</operator>
                <rightValue>
                    <booleanValue>true</booleanValue>
                </rightValue>
            </conditions>
            <connector>
                <targetReference>Create_HCP_Invoice</targetReference>
            </connector>
            <label>Invoice Does Not Exist</label>
        </rules>
    </decisions>
    <formulas>
        <name>formCB20Stipend</name>
        <dataType>Number</dataType>
        <expression>Round({!varCB20Stipend},0)</expression>
        <scale>0</scale>
    </formulas>
    <formulas>
        <name>formCity</name>
        <dataType>String</dataType>
        <expression>IF ( NOT (ISBLANK({!varOtherCity})), {!varOtherCity},{!varMailingCity})</expression>
    </formulas>
    <formulas>
        <name>formCTStipend</name>
        <dataType>Number</dataType>
        <expression>round({!varCTStipend},0)</expression>
        <scale>0</scale>
    </formulas>
    <formulas>
        <description>If Payment Schedule is quarterly, return the last day of the quarter, if the Payment Schedule is month, return the last day of the month.</description>
        <name>formInvoiceDate</name>
        <dataType>Date</dataType>
        <expression>{!varInvoiceDate} + 90</expression>
    </formulas>
    <formulas>
        <name>formPostalCode</name>
        <dataType>String</dataType>
        <expression>IF ( NOT (ISBLANK ({!varOtherZip})), {!varOtherZip}, {!varMailingPostalCode})</expression>
    </formulas>
    <formulas>
        <name>formPT20Stipend</name>
