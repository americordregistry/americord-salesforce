    <description>This is the flow that runs when the Enroll button is clicked on the opportunity.</description>
    <environments>Default</environments>
    <formulas>
        <description>Formula that will check to make sure the primary contact has been set.</description>
        <name>formBoolPrimaryContactMotherFather</name>
        <dataType>Boolean</dataType>
        <expression>AND (
OR(
AND(
ISBLANK({!recordId.Contact__r.Id}),
ISPICKVAL({!recordId.Relation_to_Baby__c}, &apos;Mother&apos;)
),
AND(
ISBLANK({!recordId.Contact2__r.Id}),
ISPICKVAL({!recordId.Contact2_Relation_to_Baby__c}, &apos;Mother&apos;)
),
AND(
ISBLANK({!recordId.Contact__r.Id}),
ISPICKVAL({!recordId.Relation_to_Baby__c}, &apos;Father&apos;),
ISPICKVAL({!recordId.Contact2_Relation_to_Baby__c}, &apos;Father&apos;)
)
)
)</expression>
    </formulas>
    <formulas>
        <description>Formula to check the referrer details are entered correctly.</description>
        <name>formBoolReferrerDetailsCheck</name>
        <dataType>Boolean</dataType>
        <expression>AND (
NOT({!$Setup.Americord_Settings__c.Bypass_Validation_Rule__c}),
NOT(ISBLANK({!recordId.Referred_by_email_address__c})) ||
NOT(ISBLANK({!recordId.Referred_by_First_Name__c})) ||
NOT(ISBLANK( {!recordId.Referred_by_Last_Name__c})),
ISBLANK ({!recordId.Referred_by_First_Name__c}) ||
ISBLANK ({!recordId.Referred_by_Last_Name__c}) ||
ISBLANK ({!recordId.Referred_by_email_address__c})
)</expression>
    </formulas>
    <formulas>
        <description>Check for special shipping label instructions.</description>
        <name>formBoolShippingLabelInstructionsCheck</name>
        <dataType>Boolean</dataType>
        <expression>AND (
NOT(ISBLANK({!recordId.Shipping_Label_Special_Instructions__c})),
{!recordId.Confirm_info_is_appropriate_for_LABEL__c} = false)</expression>
    </formulas>
    <formulas>
        <description>Formula to determine the contact2 details are set correctly.</description>
        <name>formBoolValidateContact2Details</name>
        <dataType>Boolean</dataType>
        <expression>AND (
NOT({!$Setup.Americord_Settings__c.Bypass_Validation_Rule__c}),
NOT (ISBLANK ( {!recordId.Contact2_FirstName__c})) ||
NOT (ISBLANK ( {!recordId.Contact2_LastName__c})) ||
NOT (ISBLANK (TEXT( {!recordId.Contact2_Relation_to_Baby__c}))),
ISBLANK ( {!recordId.Contact2_FirstName__c}) ||
ISBLANK ( {!recordId.Contact2_LastName__c}) ||
ISBLANK (TEXT( {!recordId.Contact2_Relation_to_Baby__c}))
)</expression>
    </formulas>
    <formulas>
        <description>Formula that checks if configured for mother or surrogate - must be one of them.</description>
        <name>formBoolValidateMotherOrSurrogate</name>
        <dataType>Boolean</dataType>
        <expression>AND(
  NOT(ISPICKVAL( {!recordId.Relation_to_Baby__c}, &apos;Mother&apos;)),
  NOT(ISPICKVAL( {!recordId.Contact2_Relation_to_Baby__c}, &apos;Mother&apos;)),
   {!recordId.Surrogate__c} = false
)</expression>
    </formulas>
