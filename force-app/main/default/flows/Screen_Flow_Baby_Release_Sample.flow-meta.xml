    <assignments>
        <description>This will set the values to compare vs. entered for validation.</description>
        <name>Set_LPI_Numbers_For_Release_Validation</name>
        <label>Set LPI Numbers For Release Validation</label>
        <locationX>578</locationX>
        <locationY>278</locationY>
        <assignmentItems>
            <assignToReference>varTLPI_CB</assignToReference>
            <operator>Assign</operator>
            <value>
                <elementReference>GetCurrentBabyRecord.LPI_CB__c</elementReference>
            </value>
        </assignmentItems>
        <assignmentItems>
            <assignToReference>varTLPI_CT</assignToReference>
            <operator>Assign</operator>
            <value>
                <elementReference>GetCurrentBabyRecord.LPI_CT__c</elementReference>
            </value>
        </assignmentItems>
        <assignmentItems>
            <assignToReference>varTLPI_PT</assignToReference>
            <operator>Assign</operator>
            <value>
                <elementReference>GetCurrentBabyRecord.LPI_PT__c</elementReference>
            </value>
        </assignmentItems>
        <assignmentItems>
            <assignToReference>varTLPI_PT20</assignToReference>
            <operator>Assign</operator>
            <value>
                <elementReference>GetCurrentBabyRecord.LPI_PT_2_0_Amnion__c</elementReference>
            </value>
        </assignmentItems>
        <connector>
            <targetReference>Release_Confirmation</targetReference>
        </connector>
    </assignments>
    <assignments>
