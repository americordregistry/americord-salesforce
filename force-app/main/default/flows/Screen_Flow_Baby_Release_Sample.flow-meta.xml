    </assignments>
    <assignments>
        <name>Set_PT_Release_True</name>
        <label>Set PT Release True</label>
        <locationX>578</locationX>
        <locationY>638</locationY>
        <assignmentItems>
            <assignToReference>GetCurrentBabyRecord.Released_PT__c</assignToReference>
            <operator>Assign</operator>
            <value>
                <booleanValue>true</booleanValue>
            </value>
        </assignmentItems>
        <connector>
            <targetReference>UpdateCurrentRecordWithReleaseValues</targetReference>
        </connector>
    </assignments>
    <decisions>
        <description>Determine if the value entered matches the current LPI.</description>
        <name>decValidateLPIMatched</name>
        <label>decValidateLPIMatched</label>
        <locationX>578</locationX>
        <locationY>518</locationY>
        <defaultConnector>
            <targetReference>Release_Confirmation_Did_Not_Match</targetReference>
        </defaultConnector>
        <defaultConnectorLabel>Entered Value Does Not Match</defaultConnectorLabel>
        <rules>
            <name>LPI_CB_Matches</name>
            <conditionLogic>and</conditionLogic>
            <conditions>
                <leftValueReference>varTLPI_CB</leftValueReference>
                <operator>EqualTo</operator>
                <rightValue>
                    <elementReference>txtCBLPIEntered</elementReference>
                </rightValue>
            </conditions>
            <conditions>
                <leftValueReference>txtCBLPIEntered</leftValueReference>
                <operator>IsNull</operator>
                <rightValue>
                    <booleanValue>false</booleanValue>
                </rightValue>
            </conditions>
            <connector>
                <targetReference>Set_CB_Release_True</targetReference>
            </connector>
            <label>LPI CB Matches</label>
        </rules>
        <rules>
            <name>LPI_CT_Matches</name>
            <conditionLogic>and</conditionLogic>
            <conditions>
                <leftValueReference>varTLPI_CT</leftValueReference>
                <operator>EqualTo</operator>
                <rightValue>
                    <elementReference>txtCTLPIEntered</elementReference>
                </rightValue>
            </conditions>
            <conditions>
                <leftValueReference>txtCTLPIEntered</leftValueReference>
                <operator>IsNull</operator>
                <rightValue>
                    <booleanValue>false</booleanValue>
                </rightValue>
            </conditions>
            <connector>
                <targetReference>Set_CT_Release_True</targetReference>
            </connector>
            <label>LPI CT Matches</label>
        </rules>
        <rules>
            <name>LPI_PT_Matches</name>
            <conditionLogic>and</conditionLogic>
            <conditions>
                <leftValueReference>varTLPI_PT</leftValueReference>
                <operator>EqualTo</operator>
                <rightValue>
                    <elementReference>txtPTLPIEntered</elementReference>
                </rightValue>
            </conditions>
            <conditions>
                <leftValueReference>txtPTLPIEntered</leftValueReference>
                <operator>IsNull</operator>
                <rightValue>
                    <booleanValue>false</booleanValue>
                </rightValue>
            </conditions>
            <connector>
                <targetReference>Set_PT_Release_True</targetReference>
            </connector>
            <label>LPI PT Matches</label>
        </rules>
        <rules>
            <name>LPI_PT_2_0_Matches</name>
            <conditionLogic>and</conditionLogic>
            <conditions>
                <leftValueReference>varTLPI_PT20</leftValueReference>
                <operator>EqualTo</operator>
                <rightValue>
                    <elementReference>txtPT20LPIEntered</elementReference>
                </rightValue>
            </conditions>
            <conditions>
                <leftValueReference>txtPT20LPIEntered</leftValueReference>
                <operator>IsNull</operator>
                <rightValue>
                    <booleanValue>false</booleanValue>
                </rightValue>
            </conditions>
            <connector>
                <targetReference>Set_PT_2_0_True</targetReference>
            </connector>
            <label>LPI PT 2.0 Matches</label>
        </rules>
    </decisions>
    <description>This flow will validate the sample that is getting released using the LPI number.</description>
    <environments>Default</environments>
    <interviewLabel>Screen Flow: Baby - Release Sample {!$Flow.CurrentDateTime}</interviewLabel>
    <label>Screen Flow: Baby - Release Sample</label>
    <processMetadataValues>
        <name>BuilderType</name>
        <value>
            <stringValue>LightningFlowBuilder</stringValue>
        </value>
    </processMetadataValues>
    <processMetadataValues>
        <name>CanvasMode</name>
        <value>
            <stringValue>AUTO_LAYOUT_CANVAS</stringValue>
        </value>
    </processMetadataValues>
    <processMetadataValues>
        <name>OriginBuilderType</name>
        <value>
            <stringValue>LightningFlowBuilder</stringValue>
        </value>
    </processMetadataValues>
    <processType>Flow</processType>
    <recordLookups>
        <description>This will get the current baby record.</description>
        <name>GetCurrentBabyRecord</name>
        <label>GetCurrentBabyRecord</label>
        <locationX>578</locationX>
        <locationY>158</locationY>
        <assignNullValuesIfNoRecordsFound>false</assignNullValuesIfNoRecordsFound>
        <connector>
            <targetReference>Set_LPI_Numbers_For_Release_Validation</targetReference>
        </connector>
        <filterLogic>and</filterLogic>
        <filters>
            <field>Id</field>
            <operator>EqualTo</operator>
            <value>
                <elementReference>recordId</elementReference>
            </value>
        </filters>
        <getFirstRecordOnly>true</getFirstRecordOnly>
        <object>Baby__c</object>
        <storeOutputAutomatically>true</storeOutputAutomatically>
    </recordLookups>
    <recordUpdates>
        <description>Updates the release based on entered LPI.</description>
        <name>UpdateCurrentRecordWithReleaseValues</name>
        <label>UpdateCurrentRecordWithReleaseValues</label>
        <locationX>578</locationX>
        <locationY>854</locationY>
        <inputReference>GetCurrentBabyRecord</inputReference>
    </recordUpdates>
    <screens>
        <name>Release_Confirmation</name>
        <label>Release Confirmation</label>
        <locationX>578</locationX>
        <locationY>398</locationY>
        <allowBack>false</allowBack>
        <allowFinish>true</allowFinish>
        <allowPause>false</allowPause>
        <connector>
            <targetReference>decValidateLPIMatched</targetReference>
