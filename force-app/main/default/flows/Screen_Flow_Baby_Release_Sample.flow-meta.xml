        </fields>
        <fields>
            <name>txtCTLPIEntered</name>
            <dataType>String</dataType>
            <fieldText>Enter CT LPI To Release</fieldText>
            <fieldType>InputField</fieldType>
            <isRequired>false</isRequired>
        </fields>
        <fields>
            <name>txtPTLPIEntered</name>
            <dataType>String</dataType>
            <fieldText>Enter PT LPI To Release</fieldText>
            <fieldType>InputField</fieldType>
            <isRequired>false</isRequired>
        </fields>
        <fields>
            <name>txtPT20LPIEntered</name>
            <dataType>String</dataType>
            <fieldText>Enter PT 2.0 To Release</fieldText>
            <fieldType>InputField</fieldType>
            <isRequired>false</isRequired>
        </fields>
        <nextOrFinishButtonLabel>Release</nextOrFinishButtonLabel>
        <showFooter>true</showFooter>
        <showHeader>true</showHeader>
    </screens>
    <screens>
        <name>Release_Confirmation_Did_Not_Match</name>
        <label>Release Confirmation - Did Not Match</label>
        <locationX>1106</locationX>
        <locationY>638</locationY>
        <allowBack>true</allowBack>
        <allowFinish>true</allowFinish>
        <allowPause>false</allowPause>
        <connector>
            <targetReference>UpdateCurrentRecordWithReleaseValues</targetReference>
        </connector>
        <fields>
            <name>DisplayDidNotMatchText</name>
            <fieldText>&lt;p&gt;One of the LPI values you entered does not match and the record will sample will not be released.&lt;/p&gt;</fieldText>
            <fieldType>DisplayText</fieldType>
        </fields>
        <nextOrFinishButtonLabel>Done</nextOrFinishButtonLabel>
        <showFooter>true</showFooter>
        <showHeader>true</showHeader>
    </screens>
