    <decisions>
        <description>Check to see if fulfillment info changed</description>
        <name>Did_Fulfillment_Info_Change</name>
        <label>Did Fulfillment Info Change?</label>
        <locationX>666</locationX>
        <locationY>974</locationY>
        <defaultConnector>
            <targetReference>Loop_over_family_member_records</targetReference>
        </defaultConnector>
        <defaultConnectorLabel>No Fulfillment Change for FM</defaultConnectorLabel>
        <rules>
            <name>Yes_Fulfillment_Changed</name>
            <conditionLogic>or</conditionLogic>
            <conditions>
                <leftValueReference>Loop_over_family_member_records.Kit_ID__c</leftValueReference>
                <operator>NotEqualTo</operator>
                <rightValue>
                    <elementReference>formTFamilyMemberClientId</elementReference>
                </rightValue>
            </conditions>
            <conditions>
                <leftValueReference>Loop_over_family_member_records.Kit_Expiration_Date__c</leftValueReference>
                <operator>NotEqualTo</operator>
                <rightValue>
                    <elementReference>varRecLab.Kit_Expiration_Date__c</elementReference>
                </rightValue>
            </conditions>
            <conditions>
                <leftValueReference>Loop_over_family_member_records.Kit_Lot_Number__c</leftValueReference>
                <operator>NotEqualTo</operator>
                <rightValue>
                    <elementReference>varRecLab.Kit_Lot_Number__c</elementReference>
                </rightValue>
            </conditions>
            <connector>
                <targetReference>Add_Family_Member_Record_To_List</targetReference>
            </connector>
            <label>Yes Fulfillment Changed</label>
        </rules>
    </decisions>
    <decisions>
        <description>Determine if we are updating for single, twins or trips based on number of births.</description>
        <name>Update_Single_Twins_or_Triplets</name>
        <label>Update Single, Twins or Triplets</label>
        <locationX>446</locationX>
        <locationY>134</locationY>
        <defaultConnectorLabel>Default Outcome</defaultConnectorLabel>
        <rules>
            <name>Twin</name>
            <conditionLogic>and</conditionLogic>
            <conditions>
                <leftValueReference>varRecLab.Number_of_Births__c</leftValueReference>
                <operator>Contains</operator>
                <rightValue>
                    <stringValue>Twins</stringValue>
                </rightValue>
            </conditions>
            <connector>
                <targetReference>Update_BabyA_Records_for_Twins</targetReference>
            </connector>
            <label>Twin</label>
        </rules>
        <rules>
            <name>Triplets</name>
            <conditionLogic>and</conditionLogic>
            <conditions>
                <leftValueReference>varRecLab.Number_of_Births__c</leftValueReference>
                <operator>EqualTo</operator>
                <rightValue>
                    <stringValue>Triplets</stringValue>
                </rightValue>
            </conditions>
            <connector>
                <targetReference>Update_BabyA_Records_for_Trips</targetReference>
            </connector>
            <label>Triplets</label>
        </rules>
        <rules>
            <name>Single</name>
            <conditionLogic>and</conditionLogic>
            <conditions>
                <leftValueReference>varRecLab.Number_of_Births__c</leftValueReference>
                <operator>EqualTo</operator>
                <rightValue>
                    <stringValue>Single</stringValue>
                </rightValue>
            </conditions>
            <connector>
                <targetReference>Update_Baby_Records_for_Single</targetReference>
            </connector>
            <label>Single</label>
        </rules>
    </decisions>
