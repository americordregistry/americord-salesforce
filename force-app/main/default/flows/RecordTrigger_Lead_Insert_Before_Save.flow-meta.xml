        <name>Is_Created_By_Sales</name>
        <label>Is Created By Sales</label>
        <locationX>446</locationX>
        <locationY>431</locationY>
        <defaultConnector>
            <targetReference>Update_Phone_to_E_164</targetReference>
        </defaultConnector>
        <defaultConnectorLabel>NO - Not created by sales</defaultConnectorLabel>
        <rules>
            <name>YES_Created_by_Sales</name>
            <conditionLogic>and</conditionLogic>
            <conditions>
                <leftValueReference>$Record.Owner:User.Department</leftValueReference>
                <operator>Contains</operator>
                <rightValue>
                    <stringValue>Sales</stringValue>
                </rightValue>
            </conditions>
            <connector>
