    <description>This flow can be used to update the lead on record insert before save.</description>
    <environments>Default</environments>
    <formulas>
        <description>Determine the acquisition program based on the Incoming_Source__c field</description>
        <name>formTAcquisitionProgram</name>
        <dataType>String</dataType>
        <expression>CASE({!$Record.Incoming_Source__c},&quot;Call&quot;,&quot;OP|2021|02|Incoming Call&quot;,&quot;Email&quot;,&quot;OP|2021|02|Incoming Email&quot;,&quot;SMS&quot;,&quot;OP|2021|02|Incoming SMS&quot;,&quot;Doctor&apos;s Office&quot;,&quot;OP|2021|02|Doctor&quot;,&quot;OP|2018|08|1274|Created in Salesforce&quot;)</expression>
    </formulas>
    <formulas>
        <description>This formula will convert the lead contact phone field to the E164 format.</description>
        <name>forTContactPhone_E164Format</name>
        <dataType>String</dataType>
        <expression>if (
    len(
        SUBSTITUTE(
            SUBSTITUTE(
                SUBSTITUTE(
                    SUBSTITUTE(
                        SUBSTITUTE(
                            SUBSTITUTE({!$Record.Contact_Phone__c}, &quot;-&quot;, &quot;&quot;), &quot; &quot;, &quot;&quot;), &quot;(&quot;, &quot;&quot;), &quot;)&quot;, &quot;&quot;), &quot;+&quot;, &quot;&quot;), &quot;.&quot;, &quot;&quot;)) = 10,
    &quot;+1&quot; +
    SUBSTITUTE(
        SUBSTITUTE(
            SUBSTITUTE(
                SUBSTITUTE(
                    SUBSTITUTE(
                        SUBSTITUTE({!$Record.Contact_Phone__c}, &quot;-&quot;, &quot;&quot;), &quot; &quot;, &quot;&quot;), &quot;(&quot;, &quot;&quot;), &quot;)&quot;, &quot;&quot;), &quot;+&quot;, &quot;&quot;), &quot;.&quot;, &quot;&quot;), {!$Record.Contact_Phone__c})</expression>
    </formulas>
    <formulas>
        <description>This formula will convert the Parent2 Phone to the E164 format.</description>
        <name>forTParent2Phone_E164Format</name>
        <dataType>String</dataType>
        <expression>if (
    len(
        SUBSTITUTE(
            SUBSTITUTE(
                SUBSTITUTE(
                    SUBSTITUTE(
                        SUBSTITUTE(
                            SUBSTITUTE({!$Record.Alt_Phone__c}, &quot;-&quot;, &quot;&quot;), &quot; &quot;, &quot;&quot;), &quot;(&quot;, &quot;&quot;), &quot;)&quot;, &quot;&quot;), &quot;+&quot;, &quot;&quot;), &quot;.&quot;, &quot;&quot;)) = 10,
    &quot;+1&quot; +
    SUBSTITUTE(
        SUBSTITUTE(
            SUBSTITUTE(
                SUBSTITUTE(
                    SUBSTITUTE(
                        SUBSTITUTE({!$Record.Alt_Phone__c}, &quot;-&quot;, &quot;&quot;), &quot; &quot;, &quot;&quot;), &quot;(&quot;, &quot;&quot;), &quot;)&quot;, &quot;&quot;), &quot;+&quot;, &quot;&quot;), &quot;.&quot;, &quot;&quot;), {!$Record.Alt_Phone__c})</expression>
    </formulas>
    <formulas>
        <description>This formula will compute the E.164 format for phone.</description>
        <name>forTPhone_E164Format</name>
        <dataType>String</dataType>
        <expression>if (
    len(
        SUBSTITUTE(
            SUBSTITUTE(
                SUBSTITUTE(
