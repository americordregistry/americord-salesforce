    <decisions>
        <name>Does_Portal_Authentication_2_Equal_Porta_Authentication_1</name>
        <label>Does Portal Authentication 2 Equal Porta Authentication 1</label>
        <locationX>448</locationX>
        <locationY>238</locationY>
        <defaultConnectorLabel>[Default Outcome]</defaultConnectorLabel>
        <rules>
            <name>Portal_Authentication_2_equals_Portal_Authentication_1</name>
            <conditionLogic>or</conditionLogic>
            <conditions>
                <leftValueReference>varPortalAuthentication2</leftValueReference>
                <operator>EqualTo</operator>
                <rightValue>
                    <elementReference>varPortalAuthentication1</elementReference>
                </rightValue>
            </conditions>
            <conditions>
                <leftValueReference>varPortalAuthentication2</leftValueReference>
                <operator>IsNull</operator>
                <rightValue>
                    <booleanValue>true</booleanValue>
                </rightValue>
            </conditions>
            <connector>
                <targetReference>Update_Contact</targetReference>
            </connector>
            <label>Portal Authentication 2 equals Portal Authentication 1</label>
        </rules>
        <rules>
            <name>Portal_Authentication_2_does_not_equal_Portal_Authentication_1</name>
            <conditionLogic>or</conditionLogic>
            <conditions>
                <leftValueReference>varPortalAuthentication2</leftValueReference>
                <operator>NotEqualTo</operator>
                <rightValue>
                    <elementReference>varPortalAuthentication1</elementReference>
                </rightValue>
            </conditions>
            <connector>
                <targetReference>Update_Contact_and_PA</targetReference>
            </connector>
            <label>Portal Authentication 2 does not equal Portal Authentication 1</label>
        </rules>
    </decisions>
