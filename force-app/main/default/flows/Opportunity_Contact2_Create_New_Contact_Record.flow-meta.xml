<?xml version="1.0" encoding="UTF-8"?>
<Flow xmlns="http://soap.sforce.com/2006/04/metadata">
    <apiVersion>49.0</apiVersion>
    <areMetricsLoggedToDataCloud>false</areMetricsLoggedToDataCloud>
    <constants>
        <name>conAcqProgram</name>
        <dataType>String</dataType>
        <value>
            <stringValue>OP|2018|08|1274|Created in Salesforce</stringValue>
        </value>
    </constants>
    <constants>
        <name>conLeadsource</name>
        <dataType>String</dataType>
        <value>
            <stringValue>Family Member</stringValue>
        </value>
    </constants>
    <description>8 Mar 2021 Added Acq Program and Lead Source = Family Member</description>
    <formulas>
        <name>formAccountName</name>
        <dataType>String</dataType>
        <expression>{!varContact2FirstName}&amp;&quot; &quot;&amp;{!varContact2LastName}</expression>
    </formulas>
    <formulas>
        <name>formOpportunitywithBabysDueDate</name>
        <dataType>String</dataType>
        <expression>{!varContact2FirstName}&amp;&quot; &quot;&amp;{!varContact2LastName}&amp;&quot; - &quot;
&amp; text(MONTH({!varBabysDueDate})) 
&amp; &quot;/&quot; 
&amp; text(DAY({!varBabysDueDate})) 
&amp; &quot;/&quot;
&amp; text(YEAR({!varBabysDueDate}))</expression>
    </formulas>
    <formulas>
        <description>If email address is provided, sync with Marketo</description>
        <name>formSyncWithMarketo</name>
        <dataType>Boolean</dataType>
        <expression>if(NOT(ISNULL({!varContact2Email})), true, false)</expression>
    </formulas>
    <formulas>
        <name>formToday</name>
        <dataType>Date</dataType>
        <expression>today()</expression>
    </formulas>
    <interviewLabel>Opportunity Contact2 Create New Contact Record {!$Flow.CurrentDateTime}</interviewLabel>
    <label>Opportunity Contact2 Create New Contact Record</label>
    <processMetadataValues>
        <name>BuilderType</name>
        <value>
            <stringValue>LightningFlowBuilder</stringValue>
        </value>
    </processMetadataValues>
    <processMetadataValues>
        <name>CanvasMode</name>
        <value>
