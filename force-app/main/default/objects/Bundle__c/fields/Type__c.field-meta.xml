    <label>Type</label>
    <required>true</required>
    <trackHistory>false</trackHistory>
    <trackTrending>false</trackTrending>
    <type>Picklist</type>
    <valueSet>
        <restricted>true</restricted>
        <valueSetDefinition>
            <sorted>false</sorted>
            <value>
                <fullName>Single-Product</fullName>
                <default>false</default>
                <label>Single-Product</label>
            </value>
            <value>
                <fullName>Multi-Product</fullName>
                <default>false</default>
                <label>Multi-Product</label>
            </value>
        </valueSetDefinition>
    </valueSet>
</CustomField>
